<view class='header-right-like'>
    <image
        wx:if='{{ like_status == 0 }}'
        src='/assets/book-par/like.png'
        bindtap='onClickLike'
        >
    </image>
    <image
        wx:else
        src='/assets/movie/like.png'
        bindtap='onClickCancel'
        >
    </image>
    <text>{{ fav_nums }}</text>
</view>